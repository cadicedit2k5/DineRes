{
  "expo": {
    "name": "DineRes",
    "slug": "dineresmobileapp",
    "scheme": "dineres",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/DineResLoGo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/DineResLoGo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/DineResLoGo.png",
        "backgroundColor": "#ffffff",
        "softwareKeyboardLayoutMode": "pan"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.dineresmobileapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font"
    ]
  }
}
